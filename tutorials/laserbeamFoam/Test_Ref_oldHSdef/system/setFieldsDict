FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

regions
(



    boxToCell
    {
        box (-0.85 0.001 -0.5) (0.85 0.004 0.5);
        fieldValues
        (
    volScalarFieldValue alpha.metal 1
        );
    }

