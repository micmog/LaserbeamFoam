FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    #includeEtc "caseDicts/setConstraintTypes"
    
    
	frontAndBack{
        type            slip;
	}
    lowerWall
    {
        type            slip;

    }
    atmosphere
    {
type pressureInletOutletVelocity;
phi phi;
rho rho;
tangentialVelocity uniform (0 0 0);
value uniform (0 0 0);
    }
rightWall
    {
        type            slip;
    }
    leftWall
    {
        type            slip;
    }
    defaultFaces
    {
        type            empty;
    }
}


