FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    #includeEtc "caseDicts/setConstraintTypes"
    
    
	frontAndBack{
        type            fixedFluxPressure;
        value           uniform 0;
	}

    lowerWall
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    atmosphere
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
    }
rightWall
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }
    leftWall
    {
        type            fixedFluxPressure;
        value           uniform 0;
    }

    defaultFaces
    {
        type            empty;
    }
}

