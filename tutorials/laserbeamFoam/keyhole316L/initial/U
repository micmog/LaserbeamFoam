FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

	#includeEtc "caseDicts/setConstraintTypes"

	frontAndBack
	{
	    type            zeroGradient;
        //type            slip;
	}
	
    lowerWall
    {

		type 			pressureInletOutletVelocity;
		phi 			phi;
		rho 			rho;
		tangentialVelocity 	uniform (0 0 0);
		value 			uniform (0 0 0.5);    
    	
        //type            slip;
        //type            zeroGradient;
    }
    
    atmosphere
    {
		type 			pressureInletOutletVelocity;
		phi 			phi;
		rho 			rho;
		tangentialVelocity 	uniform (0 0 0);
		value 			uniform (0 0 0.5);
    }
    
    leftWall
    {
    
    	type            zeroGradient;
    	
		//type 			slip;
    }
    
    rightWall
    {
    	type            zeroGradient;
    
		//type 			slip;
    }
      
}


