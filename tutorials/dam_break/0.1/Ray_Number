FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.1";
    object      Ray_Number;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform -1;

boundaryField
{
    leftWall
    {
        type            calculated;
        value           uniform -1;
    }
    rightWall
    {
        type            calculated;
        value           uniform -1;
    }
    lowerWall
    {
        type            calculated;
        value           uniform -1;
    }
    atmosphere
    {
        type            calculated;
        value           uniform -1;
    }
    defaultFaces
    {
        type            empty;
    }
}


